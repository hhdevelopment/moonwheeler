{
  "name": "@hhdev/moonwheeler",
  "version": "1.0.18",
  "author": "Achache francois",
  "license": "MIT",
  "description": "Future Electric vehicles are already here",
  "repository": {
    "type": "git",
    "url": "https://github.com/hhdevelopment/ev.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "electric vehicules",
    "electric scooter",
    "electric unicycle",
    "electric skate"
  ],
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "scripts": {
    "start:website:fr-fr": "ng serve --base-href /fr-fr/",
    "start:website:en-us": "ng serve --port 4201 --configuration=en-us --base-href /en-us/",
    "build:website:fr-fr": "ng build --outputPath dist/website/fr-fr/ --base-href /fr-fr/",
    "build:website:en-us": "ng build --outputPath dist/website/en-us/ --i18n-file src/locale/messages.en-us.xlf --i18n-format xlf --i18n-locale en-us --base-href /en-us/",
    "build:website:prod:fr-fr": "ng build --outputPath dist/website/fr-fr/ --prod --base-href /fr-fr/",
    "build:website:prod:en-us": "ng build --outputPath dist/website/en-us/ --prod --i18n-file src/locale/messages.en-us.xlf --i18n-format xlf --i18n-locale en-us --base-href /en-us/",
    "build:websites:prod": "npm run build:website:prod:fr-fr && npm run build:website:prod:en-us",
    "test:website": "ng test moonwheeler",
    "test:website:ci": "ng test moonwheeler --no-watch --no-progress --code-coverage --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "extract-i18n": "ng xi18n --i18n-locale fr-fr --output-path dist/locale",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "copy:static": "node static.copy.js",
    "build:sitemap": "node sitemap.build.js",
    "serve:ssr": "node dist/server.js",
    "build:server:fr-fr": "ng run moonwheeler:server --outputPath dist/server/fr-fr",
    "build:server:en-us": "ng run moonwheeler:server --outputPath dist/server/en-us --i18n-file src/locale/messages.en-us.xlf --i18n-format xlf --i18n-locale en-us",
    "build:servers": "npm run build:server:fr-fr && npm run build:server:en-us",
    "build:client-and-server-bundles": "npm run build:websites:prod && npm run build:servers",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "heroku-prebuild": "cd functions && npm install && cd .. && npm install",
    "heroku-postbuild": "npm run build:ssr && npm run copy:static && npm run build:sitemap",
    "docker:build": "docker build -t h2dev/moonwheeler .",
    "docker:run": "docker run -d --rm -p80:80 --name moonwheeler h2dev/moonwheeler"
  },
  "dependencies": {
    "@angular/animations": "8.2.10",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.10",
    "@angular/compiler": "8.2.10",
    "@angular/core": "8.2.10",
    "@angular/fire": "5.2.1",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "8.2.10",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.10",
    "@angular/platform-browser-dynamic": "8.2.10",
    "@angular/platform-server": "8.2.10",
    "@angular/router": "8.2.10",
    "@fortawesome/angular-fontawesome": "0.5.0",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-regular-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@hhangular/store": "1.0.15",
    "@hhangular/star-rating": "1.0.1",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "compression": "1.7.4",
    "core-js": "3.3.1",
    "express": "4.17.1",
    "firebase": "7.2.0",
    "hammerjs": "2.0.8",
    "ngx-markdown": "8.2.1",
    "prismjs": "1.17.1",
    "rxjs": "6.5.3",
    "tslib": "1.10.0",
    "ws": "7.1.2",
    "xhr2": "0.2.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.9",
    "@angular-devkit/build-ng-packagr": "0.803.9",
    "@angular/cli": "8.3.9",
    "@angular/compiler-cli": "8.2.10",
    "@angular/language-service": "8.2.10",
    "@types/jasmine": "3.4.4",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "8.10.54",
    "codecov": "3.6.2",
    "codelyzer": "5.1.2",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "ng-packagr": "5.6.1",
    "protractor": "5.4.2",
    "ts-loader": "4.5.0",
    "ts-node": "8.4.1",
    "tsickle": "0.37.0",
    "tslint": "5.20.0",
    "typescript": "3.5.3",
    "webpack-cli": "3.3.9",
    "xmlbuilder": "13.0.2"
  }
}
